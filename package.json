{
  "name": "fastest-validator",
  "version": "0.5.0",
  "description": "The fastest JS validator library for NodeJS",
  "main": "index.js",
  "scripts": {
    "bench": "node benchmark/index.js",
    "dev": "nodemon examples/index.js",
    "ci": "jest --watch",
    "test": "jest --coverage",
    "lint": "eslint --ext=.js src test",
    "deps": "npm-check -u",
    "coverall": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/icebob/fastest-validator.git"
  },
  "keywords": [
    "validation",
    "validators",
    "validate",
    "schema"
  ],
  "author": "Icebob",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/icebob/fastest-validator/issues"
  },
  "homepage": "https://github.com/icebob/fastest-validator#readme",
  "devDependencies": {
    "benchmarkify": "2.1.0",
    "coveralls": "^2.12.0",
    "eslint": "4.1.1",
    "jest": "20.0.4",
    "jest-cli": "20.0.4",
    "npm-check": "5.4.4"
  },
  "dependencies": {
    "glob": "7.1.2",
    "lodash.defaultsdeep": "4.6.0",
    "lodash.flatten": "4.4.0"
  },
  "jest": {
    "testEnvironment": "node",
    "rootDir": "./lib",
    "roots": [
      "../test"
    ]
  }
}
